{
  "family": "jira-worklog-aggregator-fargate",
  "networkMode": "awsvpc",
  "executionRoleArn": "EXECUTION_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "jira-worklog-summary-notifier",
      "image": "CONTAINER_REPOSITORY_PATH/IMAGE_NAME",
      "essential": true,
      "environment": [
        {
          "name": "JIRA_SERVER",
          "value": "JIRA_SERVER"
        },
        {
          "name": "JIRA_ID",
          "value": "JIRA_ID"
        },
        {
          "name": "JIRA_TOKEN",
          "value": "JIRA_TOKEN"
        },
        {
          "name": "USER_GROUP",
          "value": "USER_GROUP"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
